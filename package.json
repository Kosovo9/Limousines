{
  "name": "limousines-500",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:landing\" \"npm run dev:app\" \"npm run dev:server\"",
    "dev:landing": "cd frontend/landing && npm run dev",
    "dev:app": "cd frontend/app && npm run dev",
    "dev:server": "cd backend && npm run dev",
    "build": "npm run build:landing && npm run build:app",
    "build:landing": "cd frontend/landing && npm run build",
    "build:app": "cd frontend/app && npm run build",
    "start": "cd backend && npm start",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "concurrently": "^8.2.2"
  }
}
